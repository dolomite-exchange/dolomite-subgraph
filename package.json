{
  "name": "dolomite-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/dolomite-exchange/dolomite-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "create-local": "graph create coreycaplan3/dolomite-v2 --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy coreycaplan3/dolomite-v2 --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "deploy-mainnet": "graph deploy coreycaplan3/dolomite-v2 --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-staging": "graph deploy coreycaplan3/dolomite-v2 --ipfs https://api.staging.thegraph.com/ipfs/ --node https://api.staging.thegraph.com/deploy/",
    "deploy-matic": "graph deploy coreycaplan3/dolomite-v2-matic --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-mumbai": "graph deploy coreycaplan3/dolomite-v2-mumbai --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --access-token $ACCESS_TOKEN",
    "watch-local": "graph deploy coreycaplan3/dolomite-v2 --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
